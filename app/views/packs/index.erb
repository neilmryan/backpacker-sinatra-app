<h2>All Backpacker's Packs</h2>

<% @packs.each do |pack| %>

  <% if pack.image_url != "" %>
  <img style="max-width:200px; border: 2px solid; border-radius: 3%; border-color: rgb(225, 225, 172);" src="<%= pack.image_url %>" alt="pack pic">
  <% end %>
  <h2><a href='/packs/<%= pack.id %>'><%= pack.trip_name %></a></h2>
<% end %>
